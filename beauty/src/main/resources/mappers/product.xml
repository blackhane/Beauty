<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace = mapper파일의 아이디 값 -->
<mapper namespace="kr.co.beauty.dao.ProductDAO">
	<insert id="insertProduct">
		insert into `product` set
		`prodCate1`=#{prodCate1},
		`prodCate2`=#{prodCate2},
		`prodName`=#{prodName},
		`descript`=#{descript},
		`price`=#{price},
		`discount`=#{discount},
		`stock`=#{stock},
		`company`=#{company},
		`origin`=#{origin},
		`rdate`=#{rdate},
		`color`=#{color},
		`status`=#{status},
		`size`=#{size},
		`thick`=#{thick},
		`through`=#{through},
		`lining`=#{lining},
		`flex`=#{flex},
		`thumb1`=#{thumb1},
		`thumb2`=#{thumb2},
		`thumb3`=#{thumb3},
		`thumb4`=#{thumb4},
		`thumb5`=#{thumb5},
		`thumb6`=#{thumb6},
		`detail1`=#{detail1},
		`detail2`=#{detail2},
		`detail3`=#{detail3};
	</insert>
</mapper>